[
  {
    "name": "基础属性",
    "attributes": [
      {
        "text": "业务名称",
        "bindControlAttr": "context.B",
        "inCommonUse": true,
        "showType": "text"
      },
      {
        "text": "模块名称",
        "bindControlAttr": "context.M",
        "inCommonUse": true,
        "showType": "text"
      },
      {
        "text": "页面名称",
        "bindControlAttr": "name",
        "inCommonUse": true,
        "showType": "text"
      },
      {
        "text": "页面标题",
        "bindControlAttr": "title",
        "inCommonUse": true,
        "showType": "text"
      },
      {
        "text": "背景",
        "bindControlAttr": "backgroundConfig",
        "inCommonUse": true,
        "showType": "dialog",
        "config": {
          "width": "500px",
          "height": "400px",
          "content": "BackgroundSetting",
          "title": "背景设置"
        }
      },
      {
        "text": "页面高度",
        "bindControlAttr": "fit",
        "showType": "select",
        "inCommonUse": true,
        "dictionary": [
          {
            "text": "自动",
            "value": "0"
          },
          {
            "text": "自适应父容器",
            "value": "1"
          }
        ]
      },
      {
        "text": "页头显示",
        "bindControlAttr": "hasHeader",
        "showType": "boolean",
        "changeExecFunction": "resize"
      },
      {
        "text": "页脚显示",
        "bindControlAttr": "hasFooter",
        "showType": "boolean",
        "changeExecFunction": "resize"
      },
      {
        "text": "页脚底色",
        "bindControlAttr": "footerBackground",
        "showType": "select",
        "inCommonUse": true,
        "dictionary": [
          {
            "text": "透明",
            "value": "transparent"
          },
          {
            "text": "系统底色",
            "value": ""
          }
        ]
      },
      {
        "text": "页面底部安全适配",
        "bindControlAttr": "bottomSafeArea",
        "inCommonUse": true,
        "showType": "boolean"
      },
      {
        "text": "正文页边距",
        "showType": "multipleText",
        "bindControlAttr": "contentPadding",
        "config": {
          "inputs": [
            { "placeholder": "上" },
            { "placeholder": "右" },
            { "placeholder": "下" },
            { "placeholder": "左" }
          ]
        }
      },
      {
        "text": "风格",
        "showType": "select",
        "bindControlAttr": "styles",
        "inCommonUse": true,
        "dictionary": [
          {
            "text": "无",
            "value": "0"
          },
          {
            "text": "风格1",
            "value": "circular-color-block"
          }
        ]
      },
      {
        "text": "页面类型",
        "bindControlAttr": "pageType",
        "showType": "select",
        "inCommonUse": true,
        "dictionary": [
          {
            "text": "表单详情",
            "value": "form"
          },
          {
            "text": "问卷",
            "value": "question"
          },
          {
            "text": "考试",
            "value": "exam"
          },
          {
            "text": "其他",
            "value": "other"
          }
        ]
      },
      {
        "text": "表单组件文本宽度",
        "bindControlAttr": "formItemLabelWidth",
        "inCommonUse": true,
        "showType": "number"
      },
      {
        "text": "页面等级",
        "bindControlAttr": "pageLevel",
        "inCommonUse": true,
        "showType": "number"
      },
      {
        "text": "是否启用强缓存",
        "bindControlAttr": "isKeepAlive",
        "inCommonUse": true,
        "showType": "boolean"
      }
    ]
  },
  {
    "name": "高级属性",
    "attributes": [
      {
        "text": "动态属性绑定",
        "bindControlAttr": "development",
        "showType": "dialog",
        "config": {
          "content": "DesDevelopment",
          "width": "1000px",
          "height": "60vh"
        }
      },
      {
        "text": "XSS允许",
        "bindControlAttr": "xssConfig.allow",
        "showType": "boolean"
      },
      {
        "text": "是否需要登录后才能访问",
        "bindControlAttr": "needLogin",
        "showType": "boolean"
      },
      {
        "text": "页面权限",
        "bindControlAttr": "pagePrivilege",
        "showType": "dialog",
        "config": {
          "width": "700px",
          "height": "60vh",
          "content": "PagePrivilege",
          "buttonText": "设置",
          "title": "设置"
        }
      },
      {
        "text": "访问权限",
        "bindControlAttr": "dataAccessRole",
        "showType": "dialog",
        "config": {
          "width": "700px",
          "height": "60vh",
          "content": "AccessPrivilege",
          "buttonText": "设置",
          "title": "设置"
        }
      }
    ]
  }
]