        "showType": "text",
        "inCommonUse": true
      },
      {
        "text": "名称",
        "bindControlAttr": "label",
        "showType": "text",
        "changeExecFunction": "renderContext",
        "inCommonUse": true
      },
      {
        "text": "提示文字",
        "bindControlAttr": "placeholder",
        "showType": "text",
        "inCommonUse": true
      },
      {
        "text": "名称是否显示",
        "bindControlAttr": "showLabel",
        "showType": "boolean",
        "inCommonUse": true
      },
      {
        "text": "是否失效",
        "showType": "boolean",
        "bindControlAttr": "disabled",
        "inCommonUse": true
      },
      {
        "text": "是否可见",
        "showType": "boolean",
        "bindControlAttr": "visible",
        "inCommonUse": true
      },
      {
        "text": "强制隐藏(不受visible影响)",
        "bindControlAttr": "forceHidden",
        "showType": "boolean",
        "inCommonUse": true
      },
      {
        "text": "上传附件按钮是否显示",
        "showType": "boolean",
        "bindControlAttr": "visibleUpBtn",
        "inCommonUse": true
      },
      {
        "text": "上传图片按钮是否显示",
        "showType": "boolean",
        "bindControlAttr": "visibleUpImgBtn",
        "inCommonUse": true
      },
      {
        "text": "取消上下边距",
        "bindControlAttr": "tbNoPadding",
        "showType": "boolean"
      },
      {
        "text": "是否是textarea标签",
        "showType": "boolean",
        "bindControlAttr": "isTextarea",
        "inCommonUse": true
      },
      {
        "text": "textarea行数",
        "showType": "number",
        "bindControlAttr": "textareaRows",
        "showExpress": "@[isTextarea] == true",
        "config": {
          "min": 1
        },
        "inCommonUse": true
      },
      {
        "text": "描述文字",
        "showType": "text",
        "bindControlAttr": "dataCapion",
        "desc": "<p>移动端运行该组件时会在名称后追加一段备注文字。</p><p>因布局上的问题PC端暂不支持</p>",
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "高级属性",
