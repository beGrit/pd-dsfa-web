[
  {
    "name": "基础属性",
    "attributes": [
      {
        "text": "组件标识",
        "bindControlAttr": "caption",
        "showType": "text",
        "inCommonUse": true
      },
      {
        "text": "是否可见",
        "showType": "boolean",
        "bindControlAttr": "visible"
      },
      {
        "text": "强制隐藏(不受visible影响)",
        "bindControlAttr": "forceHidden",
        "showType": "boolean",
        "inCommonUse": true
      },
      {
        "text": "边距尺寸",
        "showType": "select",
        "bindControlAttr": "marginSize",
        "inCommonUse": true,
        "dictionary": [
          { "text": "小", "value": "small" },
          { "text": "中", "value": "medium" },
          { "text": "大", "value": "big" },
          { "text": "自定义", "value": "custom" }
        ]
      },
      {
        "text": "上外边距",
        "bindControlAttr": "marginTop",
        "showExpress": "@[marginSize] !== 'custom'",
        "showType": "boolean",
        "inCommonUse": true
      },
      {
        "text": "下外边距",
        "bindControlAttr": "marginBottom",
        "showExpress": "@[marginSize] !== 'custom'",
        "showType": "boolean",
        "inCommonUse": true
      },
      {
        "text": "左外边距",
        "bindControlAttr": "marginLeft",
        "showExpress": "@[marginSize] !== 'custom'",
        "showType": "boolean",
        "inCommonUse": true
      },
      {
        "text": "右外边距",
        "bindControlAttr": "marginRight",
        "showExpress": "@[marginSize] !== 'custom'",
        "showType": "boolean",
        "inCommonUse": true
      },
      {
        "text": "上内边距",
        "bindControlAttr": "paddingTop",
        "showExpress": "@[marginSize] !== 'custom'",
        "showType": "boolean",
        "inCommonUse": true
      },
      {
        "text": "下内边距",
        "bindControlAttr": "paddingBottom",
        "showExpress": "@[marginSize] !== 'custom'",
        "showType": "boolean",
        "inCommonUse": true
      },
      {
        "text": "左内边距",
        "bindControlAttr": "paddingLeft",
        "showExpress": "@[marginSize] !== 'custom'",
        "showType": "boolean",
        "inCommonUse": true
      },
      {
        "text": "右内边距",
        "bindControlAttr": "paddingRight",
        "showExpress": "@[marginSize] !== 'custom'",
        "showType": "boolean",
        "inCommonUse": true
      },
      {
        "text": "外边距(上右下左)",
        "bindControlAttr": "marginArr",
        "showExpress": "@[marginSize] === 'custom'",
        "showType": "multipleText",
        "config": {
          "inputs": [
            {
              "placeholder": "上"
            },
            {
              "placeholder": "右"
            },
            {
              "placeholder": "下"
            },
            {
              "placeholder": "左"
            }
          ]
        },
        "inCommonUse": true
      },
      {
        "text": "内边距(上右下左)",
        "bindControlAttr": "paddingArr",
        "showExpress": "@[marginSize] === 'custom'",
        "showType": "multipleText",
        "config": {
          "inputs": [
            {
              "placeholder": "上"
            },
            {
              "placeholder": "右"
            },
            {
              "placeholder": "下"
            },
            {
              "placeholder": "左"
            }
          ]
        },
        "inCommonUse": true
      },
      {
        "text": "高度是否拉伸",
        "bindControlAttr": "showModel",
        "showType": "select",
        "inCommonUse": true,
        "dictionary": [
          { "text": "自动", "value": "none" },
          { "text": "拉伸", "value": "full" }
        ]
      },
      {
        "text": "内容宽度",
        "showType": "text",
        "bindControlAttr": "mainWidth",
        "inCommonUse": true
      },
      {
        "text": "是否有阴影",
        "showType": "boolean",
        "bindControlAttr": "shadow",
        "inCommonUse": true
      },
      {
        "text": "是否可见",
        "showType": "boolean",
        "bindControlAttr": "visible",
        "inCommonUse": true
      },
      {
        "text": "背景",
        "bindControlAttr": "backgroundConfig",
        "inCommonUse": true,
        "showType": "dialog",
        "config": {
          "width": "500px",
          "height": "400px",
          "content": "BackgroundSetting",
          "title": "背景设置"
        }
      }
    ]
  },
  {
    "name": "高级属性",
    "attributes": [
      {
        "text": "动态属性绑定",
        "bindControlAttr": "development",
        "showType": "dialog",
        "config": {
          "content": "DesDevelopment",
          "width": "1000px",
          "height": "60vh"
        }
      }
    ]
  }
]
