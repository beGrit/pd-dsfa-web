        "inCommonUse": true
      },
      {
        "text": "列表行编辑立即触发提交",
        "showType": "boolean",
        "bindControlAttr": "isRowDataChangePost",
        "inCommonUse": true
      },
      {
        "text": "列表行编辑提交接口",
        "showType": "text",
        "bindControlAttr": "rowDataChangePostUrl",
        "inCommonUse": true
      },
      {
        "text": "列表行编辑提交命名空间",
        "showType": "text",
        "bindControlAttr": "rowDataChangePostNameSpace",
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "卡片列表属性",
    "attributes": [
