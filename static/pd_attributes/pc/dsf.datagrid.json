          },
          {
            "text": "上中",
            "value": "top center"
          },
          {
            "text": "上右",
            "value": "top right"
          },
          {
            "text": "中左",
            "value": "middle left"
          },
          {
            "text": "中右",
            "value": "middle right"
          },
          {
            "text": "下左",
            "value": "bottom left"
          },
          {
            "text": "下中",
            "value": "bottom center"
          },
          {
            "text": "下右",
            "value": "bottom right"
          }
        ]
      }
