        "bindControlAttr": "operateColumn.show",
        "inCommonUse": true
      },
      {
        "text": "操作列宽度",
        "showType": "text",
        "bindControlAttr": "operateColumn.width",
        "inCommonUse": true
      },
      {
        "text": "表头设置",
        "showType": "dialog",
        "bindControlAttr": "columns",
        "config": {
          "title": "表头设置",
          "content": "DesColumnsEdit",
          "width": "1000px",
          "height": "60vh",
          "slotName": "default",
          "buttonText": "设置"
