[
  {
    "name": "基础属性",
    "attributes": [
      {
        "text": "组件标识",
        "bindControlAttr": "caption",
        "showType": "text",
        "inCommonUse": true
      },
      {
        "text": "标题",
        "showType": "text",
        "bindControlAttr": "label",
        "inCommonUse": true
      },
      {
        "text": "搜索框提示文字",
        "showType": "text",
        "bindControlAttr": "searchPlaceholder",
        "inCommonUse": true
      },
      {
        "text": "自适应父容器",
        "showType": "boolean",
        "bindControlAttr": "fit",
        "inCommonUse": true
      },
      {
        "text": "按钮栏设置",
        "showType": "dialog",
        "bindControlAttr": "buttons",
        "config": {
          "content": "DesButtonEdit",
          "width": "1400px",
          "height": "60vh"
        },
        "inCommonUse": true
      },
      {
        "text": "最大按钮数量",
        "bindControlAttr": "buttonsMaxNumber",
        "showType": "number",
        "config": {
          "min": 1
        },
        "inCommonUse": true
      },
      {
        "text": "按钮栏更多按钮形态",
        "showType": "select",
        "bindControlAttr": "buttonsMoreStyle",
        "dictionary": [
          {
            "text": "图标",
            "value": "icon-text"
          },
          {
            "text": "普通按钮",
            "value": ""
          }
        ]
      },
      {
        "text": "按钮栏更多按钮文字",
        "showType": "text",
        "bindControlAttr": "buttonsMoreText"
      },
      {
        "text": "按钮栏位置",
        "bindControlAttr": "buttonBarPosition",
        "showType": "select",
        "dictionary": [
          {
            "text": "头部",
            "value": "head"
          },
          {
            "text": "高级检索右侧",
            "value": "superSearchRight"
          }
        ]
      },
      {
        "text": "按钮高级检索右侧显示模式",
        "bindControlAttr": "buttonBarPositionMode",
        "showType": "select",
        "dictionary": [
          {
            "text": "正常",
            "value": "normal"
          },
          {
            "text": "独占一列",
            "value": "column"
          }
        ]
      },
      {
        "text": "显示面板头部",
        "showType": "boolean",
        "bindControlAttr": "showHead"
      },
      {
        "text": "显示标题",
        "showType": "boolean",
        "bindControlAttr": "showTitle"
      },
      {
        "text": "是否显示检索框",
        "showType": "boolean",
        "bindControlAttr": "showSearch"
      },
      {
        "text": "是否显示checkbox列",
        "showType": "boolean",
        "bindControlAttr": "checkboxColumn.show",
        "inCommonUse": true
      },
      {
        "text": "是否仅单选",
        "showType": "boolean",
        "bindControlAttr": "isSingleChecked",
        "showExpress": "@[checkboxColumn.show]===true",
        "inCommonUse": true
      },
      {
        "text": "背景",
        "bindControlAttr": "backgroundConfig",
        "inCommonUse": true,
        "showType": "dialog",
        "config": {
          "width": "500px",
          "height": "400px",
          "content": "BackgroundSetting",
          "title": "背景设置"
        }
      }
    ]
  },
  {
    "name": "数据查询属性",
    "attributes": [
      {
        "text": "挂载后立即请求数据",
        "showType": "boolean",
        "bindControlAttr": "mountedRequestData"
      },
      {
        "text": "查询条件设置",
        "showType": "dialog",
        "bindControlAttr": "searchInfo",
        "config": {
          "title": "查询条件设置",
          "content": "DesDataGridSearch",
          "width": "1000px",
          "height": "60vh",
          "buttonText": "设置"
        },
        "inCommonUse": true
      },
      {
        "text": "高级查询设置",
        "showType": "dialog",
        "bindControlAttr": "superSearchInfo",
        "config": {
          "title": "高级查询设置",
          "content": "DesSuperSearchSetting",
          "width": "90vw",
          "height": "60vh",
          "buttonText": "设置"
        },
        "inCommonUse": true
      },
      {
        "text": "通过按钮触发高级查询",
        "showType": "boolean",
        "bindControlAttr": "superSearchButtonTrigger",
        "inCommonUse": true
      },
      {
        "text": "高级检索是否显示筛选栏",
        "showType": "boolean",
        "bindControlAttr": "showSuperSearchFilter"
      },
      {
        "text": "高级查询默认初始化行数",
        "showType": "number",
        "bindControlAttr": "superSearchDefaultRowsNumber",
        "inCommonUse": true
      },
      {
        "text": "高级查询行间隔线",
        "showType": "boolean",
        "bindControlAttr": "superSearchRowBorder"
      },
      {
        "text": "默认排序",
        "showType": "dialog",
        "bindControlAttr": "defaultOrder",
        "config": {
          "title": "排序栏设置",
          "content": "DesListOrderByEdit",
          "width": "1170px",
          "height": "60vh",
          "buttonText": "设置"
        },
        "inCommonUse": true
      },
      {
        "text": "自定义排序",
        "showType": "dialog",
        "bindControlAttr": "customerOrder",
        "config": {
          "title": "排序栏设置",
          "content": "DesCustomerOrderByEdit",
          "width": "1170px",
          "height": "60vh",
          "buttonText": "设置"
        },
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "通用属性",
    "attributes": [
      {
        "text": "数据展示方式",
        "showType": "dialog",
        "bindControlAttr": "layoutInfo",
        "config": {
          "title": "数据展示方式设置",
          "content": "DesDataGridLayoutSetting",
          "width": "1100px",
          "height": "60vh"
        },
        "inCommonUse": true
      },
      {
        "text": "数据行按钮设置",
        "showType": "dialog",
        "bindControlAttr": "rowButtons",
        "config": {
          "content": "DesButtonEdit",
          "width": "1400px",
          "height": "60vh",
          "params": {
            "isDblClickBind": true,
            "defaultShowText": false,
            "defaultButtonStyle": "icon-text"
          }
        },
        "inCommonUse": true
      },
      {
        "text": "数据行按钮最大数量",
        "bindControlAttr": "rowButtonsMaxNumber",
        "showType": "number",
        "config": {
          "min": 1
        },
        "inCommonUse": true
      },
      {
        "text": "数据行更多按钮形态",
        "showType": "select",
        "bindControlAttr": "rowButtonsMoreStyle",
        "dictionary": [
          {
            "text": "图标",
            "value": "icon-text"
          },
          {
            "text": "普通按钮",
            "value": ""
          }
        ]
      },
      {
        "text": "数据行更多按钮文字",
        "showType": "text",
        "bindControlAttr": "rowButtonsMoreText"
      },
      {
        "text": "显示分页(最多显示100条)",
        "showType": "boolean",
        "bindControlAttr": "showPagination",
        "class": "showPagination",
        "inCommonUse": true
      },
      {
        "text": "分页位置",
        "showType": "select",
        "showExpress": "@[showPagination]",
        "bindControlAttr": "paginationPosition",
        "dictionary": [
          {
            "text": "居左",
            "value": "left"
          },
          {
            "text": "居中",
            "value": "center"
          },
          {
            "text": "居右",
            "value": "right"
          }
        ],
        "inCommonUse": true
      },
      {
        "text": "分页样式",
        "showType": "select",
        "showExpress": "@[showPagination]",
        "bindControlAttr": "paginationStyle",
        "dictionary": [
          {
            "text": "样式1",
            "value": "style0"
          },
          {
            "text": "样式2",
            "value": "style1"
          },
          {
            "text": "样式3",
            "value": "style2"
          },
          {
            "text": "样式4",
            "value": "style3"
          }
        ],
        "inCommonUse": true
      },
      {
        "text": "每页显示条数(逗号分隔)",
        "showType": "text",
        "bindControlAttr": "limits",
        "dataType": "Number[]",
        "splitChart": ",",
        "changeExecFunction": "renderPagination",
        "showExpress": "@[showPagination]===true",
        "inCommonUse": true
      },
      {
        "text": "自定义分页排版(逗号分隔)",
        "showType": "text",
        "bindControlAttr": "pagerLayout",
        "showExpress": "@[showPagination]===true",
        "inCommonUse": true
      },
      {
        "text": "是否滚动加载",
        "showType": "boolean",
        "bindControlAttr": "scrollLoad",
        "showExpress": "@[showPagination]===false",
        "desc": "table表格不支持滚动加载，开启滚动加载后将隐藏table<br/>如果只有table，则此属性无效。",
        "inCommonUse": true
      },
      {
        "text": "滚动加载一次多少条数据",
        "showType": "number",
        "bindControlAttr": "scrollPageSize",
        "showExpress": "@[showPagination]===false && @[scrollLoad]===true",
        "config": {
          "min": 1,
          "max": 200
        },
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "table属性",
    "attributes": [
      {
        "text": "简化输出",
        "showType": "boolean",
        "bindControlAttr": "tableTextProxy",
