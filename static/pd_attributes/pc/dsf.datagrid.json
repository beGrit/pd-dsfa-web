      },
      {
        "text": "是否显示索引列",
        "showType": "boolean",
        "bindControlAttr": "indexColumn.show",
        "inCommonUse": true
      },
      {
        "text": "索引列宽度",
        "showType": "text",
        "bindControlAttr": "indexColumn.width"
      },
      {
        "text": "是否显示操作按钮",
