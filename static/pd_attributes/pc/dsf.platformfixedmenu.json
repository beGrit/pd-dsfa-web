[
  {
    "name": "基础属性",
    "attributes": [
      {
        "text": "组件标识",
        "bindControlAttr": "caption",
        "showType": "text",
        "inCommonUse": true
      },
      {
        "text": "页面宽度",
        "bindControlAttr": "pageWidth",
        "showType": "text",
        "inCommonUse": true
      },
      {
        "text": "背景",
        "bindControlAttr": "backgroundConfig",
        "inCommonUse": true,
        "showType": "dialog",
