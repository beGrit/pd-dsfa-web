        "inCommonUse": true,
        "dictionary": [
          {
            "text": "无",
            "value": "-"
          },
          {
            "text": "特效一",
            "value": "animate1"
          }
        ]
      },
      {
        "text": "展示优先级",
        "bindControlAttr": "showPriority",
        "showType": "select",
        "inCommonUse": true,
        "dictionary": [
          {
            "text": "图片优先",
            "value": "img"
          },
          {
            "text": "icon优先",
            "value": "icon"
          }
        ]
