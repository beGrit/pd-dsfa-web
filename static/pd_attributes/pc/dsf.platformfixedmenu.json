        "bindControlAttr": "menuTextSize",
        "showType": "text",
        "inCommonUse": true
      },
      {
        "text": "按钮尺寸",
        "bindControlAttr": "menuBtSize",
        "showType": "text",
        "inCommonUse": true
      },
      {
        "text": "按钮圆角",
        "bindControlAttr": "menuBtRadius",
        "showType": "text",
        "inCommonUse": true
