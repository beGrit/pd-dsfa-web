          "width": "500px",
          "height": "400px",
          "content": "BackgroundSetting",
          "title": "背景设置"
        }
      },
      {
        "text": "位置",
        "bindControlAttr": "position",
        "inCommonUse": true,
        "showType": "select",
        "dictionary": [
          {
            "text": "居左",
            "value": "left"
          },
          {
            "text": "居右",
            "value": "right"
          }
        ]
      },
      {
        "text": "是否强制使用默认样式",
        "bindControlAttr": "useDefaultStyle",
        "showType": "boolean",
        "inCommonUse": true
      },
      {
        "text": "数据请求地址",
        "bindControlAttr": "dataSourceUrl",
        "showType": "text",
        "inCommonUse": true
      },
      {
        "text": "追加按钮",
        "bindControlAttr": "dataSourceList",
        "showType": "dialog",
        "inCommonUse": true,
        "config": {
          "content": "DesMobileNavMenuData",
          "showType": true,
          "width": "1400px",
          "height": "60vh"
        }
      }
    ]
  },
