[{
    "name": "基础属性",
    "attributes": [{
        "text": "业务名称",
        "bindControlAttr": "context.B",
        "inCommonUse": true,
        "showType": "text"
      },
      {
        "text": "模块名称",
        "bindControlAttr": "context.M",
        "inCommonUse": true,
        "showType": "text"
      },
      {
        "text": "页面名称",
        "bindControlAttr": "name",
        "inCommonUse": true,
        "showType": "text"
      },
      {
        "text": "页面标题",
        "bindControlAttr": "title",
        "inCommonUse": true,
        "showType": "text"
      },
      {
        "text": "背景",
        "bindControlAttr": "backgroundConfig",
        "inCommonUse": true,
        "showType": "dialog",
        "config": {
          "width": "500px",
          "height": "400px",
          "content": "BackgroundSetting",
          "title": "背景设置"
        }
      },
      {
        "text": "页面宽度",
        "bindControlAttr": "pageWidth",
        "inCommonUse": true,
        "showType": "text"
      },
      {
        "text": "页面高度",
        "bindControlAttr": "fit",
        "showType": "select",
        "dictionary": [{
            "text": "自动",
            "value": "0"
          },
          {
            "text": "自适应父容器",
            "value": "1"
          }
        ]
      },
      {
        "text": "页头页脚宽度",
        "bindControlAttr": "headfootwidth",
        "showType": "select",
        "showExpress": "@[1==2]",
        "dictionary": [{
            "text": "跟随页面宽度",
            "value": "same"
          },
          {
            "text": "跟随窗体宽度",
            "value": "full"
          }
        ]
      },
      {
        "text": "页头显示",
        "bindControlAttr": "hasHeader",
        "showType": "boolean",
        "changeExecFunction": "resize"
      },
      {
        "text": "页脚显示",
        "bindControlAttr": "hasFooter",
        "showType": "boolean",
        "changeExecFunction": "resize"
      },
      {
        "text": "页脚底色",
        "bindControlAttr": "footerBackground",
        "showType": "select",
        "dictionary": [{
            "text": "透明",
            "value": "transparent"
          },
          {
            "text": "系统底色",
            "value": ""
          }
        ]
      },
      {
        "text": "上下间隔",
        "bindControlAttr": "topBottom",
        "showType": "select",
        "dictionary": [{
            "text": "无",
            "value": ""
          },
          {
            "text": "小(5px)",
            "value": "split-small"
          },
          {
            "text": "中(10px)",
            "value": "split-medium"
          },
          {
            "text": "大(15px)",
            "value": "split-big"
          },
          {
            "text": "超大(20px)",
            "value": "split-highbig"
          }
        ]
      },
      {
        "text": "列间隔",
        "bindControlAttr": "leftRight",
        "showType": "select",
        "dictionary": [{
            "text": "无",
            "value": ""
          },
          {
            "text": "小(5px)",
            "value": "split-small"
          },
          {
            "text": "中(10px)",
            "value": "split-medium"
          },
          {
            "text": "大(15px)",
            "value": "split-big"
          },
          {
            "text": "超大(20px)",
            "value": "split-highbig"
          }
        ]
      },
      {
        "text": "左右视图宽度",
        "bindControlAttr": "viewSplit",
        "showType": "text",
        "inCommonUse": true,
        "changeExecFunction": "resize"
      },
      {
        "text": "正文页边距",
        "showType": "multipleText",
        "bindControlAttr": "contentPadding",
        "config": {
          "inputs": [
            { "placeholder": "上" },
            { "placeholder": "右" },
            { "placeholder": "下" },
            { "placeholder": "左" }
          ]
        }
      },
      {
        "text": "页面类型",
        "bindControlAttr": "pageType",
        "showType": "select",
        "inCommonUse": true,
        "dictionary": [{
            "text": "表单详情",
            "value": "form"
          },
          {
            "text": "问卷",
            "value": "question"
          },
          {
            "text": "考试",
            "value": "exam"
          },
          {
            "text": "其他",
            "value": "other"
          }
        ]
      },
      {
        "text": "页脚是否固定在底部",
        "bindControlAttr": "footFixed",
        "showType": "boolean",
        "desc": "当页面位于page.html时有效"
      },
      {
        "text": "表单组件文本宽度",
        "bindControlAttr": "formItemLabelWidth",
        "inCommonUse": true,
        "showType": "number"
      }
    ]
  },
  {
    "name": "高级属性",
    "attributes": [{
        "text": "XSS允许",
        "bindControlAttr": "xssConfig.allow",
        "showType": "boolean"
      },
      {
        "text": "是否需要登录后才能访问",
        "bindControlAttr": "needLogin",
        "showType": "boolean"
      },
      {
        "text": "页面权限",
        "bindControlAttr": "pagePrivilege",
        "showType": "dialog",
        "config": {
          "width": "700px",
          "height": "60vh",
          "content": "PagePrivilege",
          "buttonText": "设置",
          "title": "设置"
        }
      },
      {
        "text": "访问权限",
        "bindControlAttr": "dataAccessRole",
        "showType": "dialog",
        "config": {
          "width": "700px",
          "height": "60vh",
          "content": "AccessPrivilege",
          "buttonText": "设置",
          "title": "设置"
        }
      },
      {
        "text": "动态属性绑定",
        "bindControlAttr": "development",
        "showType": "dialog",
        "config": {
          "content": "DesDevelopment",
          "width": "1000px",
          "height": "60vh"
        }
      }
    ]
  }
]