[
  {
    "name": "基础属性",
    "attributes": [
      {
        "text": "高度",
        "bindControlAttr": "height",
        "showType": "text",
        "changeExecFunction": "onResize",
        "desc": ""
      },
      {
        "text": "接口",
        "bindControlAttr": "api",
        "showType": "text",
        "changeExecFunction": "loadData",
        "desc": ""
      },
      {
        "text": "图表类型",
        "bindControlAttr": "chartType",
        "showType": "select",
        "changeExecFunction": "clear",
        "dictionary": [
          { "text": "折线图", "value": "line" },
          { "text": "柱状图", "value": "bar" },
          { "text": "饼图", "value": "pie" },
          { "text": "环形图", "value": "circle" }
        ]
      },
      {
        "text": "系列名称",
        "bindControlAttr": "seriesName",
        "changeExecFunction": "rerender",
        "showType": "text",
        "desc": ""
      },
      {
        "text": "颜色",
        "bindControlAttr": "colors",
        "changeExecFunction": "rerender",
        "showType": "text",
        "desc": ""
      },
      {
        "text": "提示框背景颜色",
        "bindControlAttr": "tipBackground",
        "changeExecFunction": "rerender",
        "showType": "text",
        "desc": ""
      },
      {
        "text": "提示框边框颜色",
        "bindControlAttr": "tipBorder",
        "changeExecFunction": "rerender",
        "showType": "text",
        "desc": ""
      },
      {
        "text": "提示框文字颜色",
        "bindControlAttr": "tipColor",
        "changeExecFunction": "rerender",
        "showType": "text",
        "desc": ""
      },
      {
        "text": "Tooltip Formatter",
        "bindControlAttr": "tooltipFormatter",
        "changeExecFunction": "rerender",
        "showType": "text",
        "desc": ""
      },
      {
        "text": "坐标轴指示器背景",
        "bindControlAttr": "tipAsisColor",
        "changeExecFunction": "rerender",
        "showExpress": "['line', 'bar'].includes(@[chartType])",
        "showType": "text",
        "desc": ""
      },
      {
        "text": "坐标轴边距(上,右,下,左)",
        "bindControlAttr": "grid",
        "changeExecFunction": "rerender",
        "showExpress": "['line', 'bar'].includes(@[chartType])",
        "showType": "text",
        "desc": ""
      },
      {
        "text": "是否平滑显示",
        "showType": "boolean",
        "bindControlAttr": "smooth",
        "showExpress": "['line', 'bar'].includes(@[chartType])",
        "changeExecFunction": "rerender"
      },
      {
        "text": "柱形宽度",
        "bindControlAttr": "barWidth",
        "changeExecFunction": "rerender",
        "showExpress": "@[chartType] === 'bar'",
        "showType": "text",
        "desc": ""
      },
      {
        "text": "半径",
        "showType": "text",
        "bindControlAttr": "radius",
        "showExpress": "@[chartType] === 'pie'",
        "changeExecFunction": "rerender"
      },
      {
        "text": "半径",
        "showType": "text",
        "bindControlAttr": "circleRadius",
        "showExpress": "@[chartType] === 'circle'",
        "changeExecFunction": "rerender"
      },
      {
        "text": "是否显示X轴分割线",
        "showType": "boolean",
        "bindControlAttr": "showXLine",
        "showExpress": "['line', 'bar'].includes(@[chartType])",
        "changeExecFunction": "rerender"
      },
      {
        "text": "是否显示Y轴分割线",
        "showType": "boolean",
        "bindControlAttr": "showYLine",
        "showExpress": "['line', 'bar'].includes(@[chartType])",
        "changeExecFunction": "rerender"
      },
      {
        "text": "是否显示X轴分割区域",
        "showType": "boolean",
        "bindControlAttr": "showXArea",
        "showExpress": "['line', 'bar'].includes(@[chartType])",
        "changeExecFunction": "rerender"
      },
      {
        "text": "是否显示Y轴分割区域",
        "showExpress": "['line', 'bar'].includes(@[chartType])",
        "showType": "boolean",
        "bindControlAttr": "showYArea",
        "changeExecFunction": "rerender"
      },
      {
        "text": "是否显示X轴刻度",
        "showType": "boolean",
        "showExpress": "['line', 'bar'].includes(@[chartType])",
        "bindControlAttr": "axisXTick",
        "changeExecFunction": "rerender"
      },
      {
        "text": "是否显示Y轴刻度",
        "showType": "boolean",
        "showExpress": "['line', 'bar'].includes(@[chartType])",
        "bindControlAttr": "axisYTick",
        "changeExecFunction": "rerender"
      },
      {
        "text": "X轴线颜色",
        "showType": "text",
        "bindControlAttr": "axisXLineColor",
        "showExpress": "['line', 'bar'].includes(@[chartType])",
        "changeExecFunction": "rerender"
      },
      {
        "text": "X轴文字颜色",
        "showType": "text",
        "bindControlAttr": "axisXLabelColor",
        "showExpress": "['line', 'bar'].includes(@[chartType])",
        "changeExecFunction": "rerender"
      },
      {
        "text": "Y轴线颜色",
        "showType": "text",
        "bindControlAttr": "axisYLineColor",
        "showExpress": "['line', 'bar'].includes(@[chartType])",
        "changeExecFunction": "rerender"
      },
      {
        "text": "Y轴文字颜色",
        "showType": "text",
        "bindControlAttr": "axisYLabelColor",
        "showExpress": "['line', 'bar'].includes(@[chartType])",
        "changeExecFunction": "rerender"
      },
      {
        "text": "是否显示文本标签",
        "showType": "boolean",
        "bindControlAttr": "showSeriesLable",
        "changeExecFunction": "rerender"
      },
      {
        "text": "是否高亮显示文本标签",
        "showType": "boolean",
        "bindControlAttr": "showEmphasisSeriesLable",
        "showExpress": "['pie', 'circle'].includes(@[chartType])",
        "changeExecFunction": "rerender"
      },
      {
        "text": "文本标签文字颜色",
        "showType": "text",
        "bindControlAttr": "seriesLabelColor",
        "changeExecFunction": "rerender"
      },
      {
        "text": "文本标签位置",
        "bindControlAttr": "seriesLabelPosition",
        "showType": "text",
        "changeExecFunction": "rerender"
      },
      {
        "text": "是否显示视觉引导线",
        "showType": "boolean",
        "bindControlAttr": "showLabelLine",
        "showExpress": "['pie', 'circle'].includes(@[chartType])",
        "changeExecFunction": "rerender"
      }
    ]
  },
  {
    "name": "高级属性",
    "attributes": [
      {
        "text": "动态属性绑定",
        "bindControlAttr": "development",
        "showType": "dialog",
        "config": {
          "content": "DesDevelopment",
          "width": "1000px",
          "height": "60vh"
        }
      }
    ]
  }
]
